{
  "name": "qmailo-frontend",
  "version": "1.3.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run ng -- serve --proxy-config proxy.conf.json --aot",
    "start:de": "npm run start -- --i18n-file=src/i18n/messages.de.xlf --locale=de --i18n-format=xlf",
    "start:sw": "npm run build && npm run start:liteserver",
    "start:liteserver": "lite-server -c liteserver_config.js",
    "build": "ng build --prod",
    "build:en": "npm run build -- --output-path=qmailo-frontend_v%npm_package_version%_en",
    "build:de": "npm run build -- --i18n-file=src/i18n/messages.de.xlf --i18n-locale=de --i18n-format=xlf --output-path=qmailo-frontend_v%npm_package_version%_de",
    "release": "npm run build:en && npm run build:de",
    "i18n": "ng xi18n --output-path src/i18n --i18n-locale de && xliffmerge --profile xliffmerge.json de",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^10.1.2",
    "@angular-devkit/schematics": "^10.1.2",
    "@angular/animations": "10.1.2",
    "@angular/cdk": "^10.2.1",
    "@angular/common": "10.1.2",
    "@angular/compiler": "10.1.2",
    "@angular/core": "10.1.2",
    "@angular/forms": "10.1.2",
    "@angular/localize": "^10.1.2",
    "@angular/material": "^10.2.1",
    "@angular/platform-browser": "10.1.2",
    "@angular/platform-browser-dynamic": "10.1.2",
    "@angular/router": "10.1.2",
    "@angular/service-worker": "10.1.2",
    "@ngrx/effects": "^10.0.0",
    "@ngrx/entity": "^10.0.0",
    "@ngrx/store": "^10.0.0",
    "@ngrx/store-devtools": "^10.0.0",
    "@schematics/angular": "^10.1.2",
    "core-js": "^3.6.5",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.2",
    "@angular/cli": "^10.1.2",
    "@angular/compiler-cli": "10.1.2",
    "@angular/language-service": "10.1.2",
    "@ngrx/schematics": "^10.0.0",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lite-server": "^2.5.4",
    "ngx-i18nsupport": "^0.17.1",
    "protractor": "~7.0.0",
    "ts-node": "^9.0.0",
    "tslint": "~6.1.3",
    "typescript": "4.0.2"
  }
}
